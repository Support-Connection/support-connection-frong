<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Jasaninput_activity">

    <Button
        android:id="@+id/beforebtn14"
        android:layout_width="17dp"
        android:layout_height="15.8dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="56dp"
        android:background="@mipmap/before_foreground"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/jasan_top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="90dp"
        android:text="💬"
        android:textColorLink="#454545"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/jasan_text1"
        style="@style/reg_style1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/jasan_text1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_top"
        app:layout_constraintTop_toBottomOf="@+id/jasan_top"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/jasan_text2"
        style="@style/reg_style2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="@string/jasan_text2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_text1"
        app:layout_constraintTop_toBottomOf="@+id/jasan_text1"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/jasan_edit1"
        android:theme="@style/EditStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:ems="10"
        android:fontFamily="@font/noto_sans_bold"
        android:hint="연소득"
        android:inputType="number"
        android:textColor="#454545"
        android:textColorHint="#747f89"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_tag1"
        app:layout_constraintTop_toBottomOf="@+id/jasan_tag1"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/jasan_edit2"
        android:theme="@style/EditStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:ems="10"
        android:fontFamily="@font/noto_sans_bold"
        android:hint="자산규모"
        android:inputType="number"
        android:textColor="#454545"
        android:textColorHint="#747f89"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_icon2"
        app:layout_constraintTop_toBottomOf="@+id/jasan_icon2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/jasan_text3"
        style="@style/reg_style1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="원"
        app:layout_constraintBottom_toBottomOf="@+id/jasan_edit1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/jasan_edit1"
        app:layout_constraintTop_toTopOf="@+id/jasan_edit1" />

    <TextView
        android:id="@+id/jasan_text4"
        style="@style/reg_style1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="원"
        app:layout_constraintBottom_toBottomOf="@+id/jasan_edit2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/jasan_edit2"
        app:layout_constraintTop_toTopOf="@+id/jasan_edit2" />

    <Button
        android:id="@+id/jasan_btn1"
        style="@style/BtnStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="198dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/btnbackground"
        android:fontFamily="@font/noto_sans_bold"
        android:text="다음으로"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_edit2"
        app:layout_constraintTop_toBottomOf="@+id/jasan_edit2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/jasan_tag1"
        style="@style/tagstyle"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginTop="25dp"
        android:background="@drawable/nametag"
        android:text="연 소득"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_text2"
        app:layout_constraintTop_toBottomOf="@+id/jasan_text2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/jasan_icon2"
        style="@style/tagstyle"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_marginTop="26dp"
        android:background="@drawable/nametag"
        android:text="자산 규모"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/jasan_edit1"
        app:layout_constraintTop_toBottomOf="@+id/jasan_edit1"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scConstraint"
    android:layout_width="match_parent"
    android:layout_height="325dp">

    <TextView
        android:id="@+id/sc_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/noto_sans_bold"
        android:text="신청하기"
        android:textColor=" #454545"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/sc_close"
        android:layout_width="13dp"
        android:layout_height="13dp"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="25dp"
        android:layout_marginRight="25dp"
        android:background="@mipmap/close_image_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/sc_text"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/sc_icon1"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginTop="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sc_text"
        app:layout_constraintTop_toBottomOf="@+id/sc_text"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_call_foreground" />

    <ImageView
        android:id="@+id/sc_icon2"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginTop="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sc_icon1"
        app:layout_constraintTop_toBottomOf="@+id/sc_icon1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_com_foreground" />

    <ImageView
        android:id="@+id/sc_icon3"
        android:layout_width="41dp"
        android:layout_height="41dp"
        android:layout_marginTop="31dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/sc_icon2"
        app:layout_constraintTop_toBottomOf="@+id/sc_icon2"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@mipmap/ic_go_foreground" />

    <TextView
        android:id="@+id/sc_text1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/noto_sans_bold"
        android:text="전화 신청"
        android:textColor="#4e5968"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/sc_icon1"
        app:layout_constraintTop_toTopOf="@+id/sc_icon1"
        app:layout_constraintVertical_bias="0.0"
        app:lineHeight="27sp" />

    <TextView
        android:id="@+id/sc_text2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/noto_sans_bold"
        android:text="인터넷 신청"
        android:textColor="#4e5968"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/sc_icon2"
        app:layout_constraintTop_toTopOf="@+id/sc_icon2"
        app:layout_constraintVertical_bias="0.0"
        app:lineHeight="27sp" />

    <TextView
        android:id="@+id/sc_text3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="7dp"
        android:fontFamily="@font/noto_sans_bold"
        android:text="방문 신청"
        android:textColor="#4e5968"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/sc_icon3"
        app:layout_constraintTop_toTopOf="@+id/sc_icon3"
        app:layout_constraintVertical_bias="0.0"
        app:lineHeight="27sp" />
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/SwipeupContainer"
    android:layout_width="match_parent"
    android:layout_height="438dp"
    android:orientation="vertical"
    app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior">

    <TextView
        android:id="@+id/tel_text"
        android:layout_width="316dp"
        android:layout_height="54dp"
        android:fontFamily="@font/noto_sans_bold"
        android:gravity="bottom"
        android:paddingLeft="25dp"
        android:text="통신사 선택"
        android:textColor="#454545"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/close_btn"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginTop="27dp"
        android:background="@mipmap/close_image_foreground"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tel_text"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="382dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tel_text">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/TelecomConstraint"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tel_text1"
                style="@style/tel_style"
                android:layout_width="0dp"
                android:layout_height="54dp"
                android:layout_marginStart="25dp"
                android:layout_marginLeft="25dp"
                android:text="SKT"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tel_img1"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tel_text2"
                style="@style/tel_style"
                android:layout_width="0dp"
                android:layout_height="54dp"
                android:text="KT"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tel_img2"
                app:layout_constraintStart_toStartOf="@+id/tel_text1"
                app:layout_constraintTop_toBottomOf="@+id/tel_text1"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tel_text3"
                style="@style/tel_style"
                android:layout_width="0dp"
                android:layout_height="54dp"
                android:text="LGU+"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tel_img3"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/tel_text2"
                app:layout_constraintTop_toBottomOf="@+id/tel_text2"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tel_text4"
                style="@style/tel_style"
                android:layout_width="0dp"
                android:layout_height="54dp"
                android:text="알뜰폰 (SKT)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tel_img4"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/tel_text3"
                app:layout_constraintTop_toBottomOf="@+id/tel_text3"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tel_text5"
                style="@style/tel_style"
                android:layout_width="0dp"
                android:layout_height="54dp"
                android:text="알뜰폰 (KT)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tel_img5"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/tel_text4"
                app:layout_constraintTop_toBottomOf="@+id/tel_text4"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tel_text6"
                style="@style/tel_style"
                android:layout_width="0dp"
                android:layout_height="54dp"
                android:text="알뜰폰 (LGU+)"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/tel_img6"
                app:layout_constraintStart_toStartOf="@+id/tel_text5"
                app:layout_constraintTop_toBottomOf="@+id/tel_text5"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/tel_img1"
                android:layout_width="29dp"
                android:layout_height="29dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.07"
                app:srcCompat="@mipmap/selected_image_foreground" />

            <ImageView
                android:id="@+id/tel_img2"
                android:layout_width="29dp"
                android:layout_height="29dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tel_img1"
                app:layout_constraintVertical_bias="0.083"
                app:srcCompat="@mipmap/selected_image_foreground" />

            <ImageView
                android:id="@+id/tel_img3"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tel_img2"
                app:layout_constraintVertical_bias="0.109"
                app:srcCompat="@mipmap/selected_image_foreground" />

            <ImageView
                android:id="@+id/tel_img4"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tel_img3"
                app:layout_constraintVertical_bias="0.145"
                app:srcCompat="@mipmap/selected_image_foreground" />

            <ImageView
                android:id="@+id/tel_img5"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tel_img4"
                app:layout_constraintVertical_bias="0.195"
                app:srcCompat="@mipmap/selected_image_foreground" />

            <ImageView
                android:id="@+id/tel_img6"
                android:layout_width="27dp"
                android:layout_height="27dp"
                android:layout_marginEnd="20dp"
                android:layout_marginRight="20dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tel_img5"
                app:layout_constraintVertical_bias="0.321"
                app:srcCompat="@mipmap/selected_image_foreground" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>